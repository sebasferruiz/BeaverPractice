# Multiplicaci√≥n de matrices

def maxmult (a, b):
  r = [[0 for row in range(len(a))] for col in range(len(b[0]))]
  for i in range(len(a)):
    for j in range(len(b[0])):
      for k in range(len(b)):
        r[i][j] += a[i][k]*b[k][j]
        
	return r

i = [1,4,3],[3,2,3],[4,2,5]
j = [5,0,3],[3,9,3],[6,5,2]
matrixmult(i,j)
