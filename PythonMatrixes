# Multiplicaci√≥n de matrices

def matrixmult (A, B):
  C = [[0 for row in range(len(A))] for col in range(len(B[0]))]
  for i in range(len(A)):
    for j in range(len(B[0])):
      for k in range(len(B)):
        C[i][j] += A[i][k]*B[k][j]
        
return C

i = [1,4,3],[3,2,3],[4,2,5]
j = [5,0,3],[3,9,3],[6,5,2]
matrixmult(i,j)
